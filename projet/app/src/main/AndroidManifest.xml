<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.greencity">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_wtht_leaf"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_wtht_leaf"
        android:supportsRtl="true"
        android:theme="@style/Theme.GreenCity">
        <activity
            android:name=".info_recyclage"
            android:label="@string/title_activity_info_recyclage"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>